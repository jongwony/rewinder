{
    "name": "rewinder",
    "version": "0.1",
    "description": "Local browser history search Chrome Extension",
    "manifest_version": 2,
    "permissions": [
        "<all_urls>",
        "contextMenus",
        "storage",
        "webRequest",
        "webNavigation"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [{
        "matches": ["https://stackoverflow.com/*"],
        "js": ["contentScript.js"],
        "run_at": "document_start"
    }]
}
